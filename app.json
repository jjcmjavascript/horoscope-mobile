{
  "expo": {
    "name": "horoscopoSP",
    "slug": "horoscopoSP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/shared/assets/moon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/shared/assets/moon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jgintoki.horoscopemobile",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/shared/assets/moon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app/shared/assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./app/shared/assets/fonts/Moonlight.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0f1ca582-93c1-4c8d-827c-344391e6a658"
      }
    }
  }
}
