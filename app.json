{
  "expo": {
    "name": "horoscope-mobile",
    "slug": "horoscope-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./shared/assets/moon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "react-native-google-mobile-ads": {
      "android_app_id": "ca-app-pub-1322985027681766~9107981671"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jgintoki.horoscopemobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./shared/assets/moon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jgintoki.horoscopemobile",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./shared/assets/moon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./shared/assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./shared/assets/moon96.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-1322985027681766~9107981671"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0f1ca582-93c1-4c8d-827c-344391e6a658"
      }
    }
  }
}
